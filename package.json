{
  "homepage": "https://tuanbt.github.io/cocvuong/",
  "name": "react-seed",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.16.2",
    "events": "^3.3.0",
    "firebase": "^8.2.3",
    "gh-pages": "^3.2.3",
    "jquery": "^3.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-file-input": "^0.2.5",
    "react-intl": "^2.3.0",
    "react-intl-cra": "^0.2.7",
    "react-intl-po": "^2.1.1",
    "react-notifications": "^1.7.3",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-scripts": "^1.0.10",
    "redux": "^3.7.1",
    "redux-actions": "^2.2.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  },
  "engines": {
    "node": "16.15.1",
    "npm": "8.11.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "extract": "react-intl-cra 'src/**/*.jsx' 'output/messages.json'",
    "extract:pot": "react-intl-po json2pot 'output/messages.json' -o 'output/messages.pot'",
    "extract:pot-c": "react-intl-po json2pot 'output/messages.json' -c 'id' -o 'output/messages-c.pot'",
    "extract:po-c": "react-intl-po po2json 'input/*.po' -c 'id' -m 'output/messages.json' -o 'output/translation.json'"
  },
  "devDependencies": {
    "react-scripts": "latest"
  }
}
